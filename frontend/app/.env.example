# Environment Configuration
NEXT_PUBLIC_NODE_ENV=development

# Database Configuration
MONGO_URI=your_mongodb_connection_string_here

# Server URLs
# Production
NEXT_PUBLIC_API_BASE_URL_PROD=https://your-production-domain.com/api
NEXT_PUBLIC_API_BASE_URL_SOCKET_PROD=https://your-production-domain.com

# Development
NEXT_PUBLIC_API_BASE_URL=http://localhost:5000/api
NEXT_PUBLIC_API_BASE_URL_SOCKET=http://localhost:5000

# Frontend URL
NEXT_PUBLIC_FRONTEND_URL=https://your-frontend-domain.com

# Movie Database (TMDB)
MOVIE_DB_API_KEY=your_tmdb_api_key_here
TMDB_BEARER_TOKEN=your_tmdb_bearer_token_here

# Cloudinary Configuration
CLOUDINARY_URL=cloudinary://your_cloudinary_api_key:your_cloudinary_api_secret@your_cloudinary_cloud_name
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name_here
NEXT_PUBLIC_CLOUDINARY_API_KEY=your_cloudinary_api_key_here
NEXT_PUBLIC_CLOUDINARY_API_SECRET=your_cloudinary_api_secret_here

# Google Authentication
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here
NEXT_PUBLIC_GOOGLE_CLIENT_SECRET=your_google_client_secret_here
NEXTAUTH_SECRET=your_nextauth_secret_here

# Razorpay Configuration
NEXT_PUBLIC_RAZORPAY_KEY_ID=your_razorpay_key_id_here
